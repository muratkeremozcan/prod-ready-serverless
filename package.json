{
  "name": "prod-ready-sls",
  "version": "1.0.0",
  "description": "An event driven serverless app in NodeJs with API Gateway, Lambda, Cognito, EventBridge, DDB etc.",
  "author": "Murat Ozcan",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "get:branch": "git rev-parse --abbrev-ref HEAD",
    "sls": "sls",
    "deploy": "sls deploy",
    "deploy:branch": "BRANCH=$(npm run get:branch | awk 'END{print}') && sls deploy -s $BRANCH",
    "deploy:stage": "sls deploy -s stage",
    "export:env": "sls export-env --all",
    "export:env-stage": "sls export-env -s stage --all",
    "export:env-branch": "BRANCH=$(npm run get:branch | awk 'END{print}') && npm run sls export-env -- -s $BRANCH --all",
    "remove:branch": "BRANCH=$(npm run get:branch | awk 'END{print}') && npm run sls -- remove -s $BRANCH",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|css|md|mdx|html)\"",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "test": "jest --silent",
    "cypress": "cypress open",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e --browser chrome"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.363.0",
    "@aws-sdk/util-dynamodb": "3.365.0",
    "@types/jest": "29.2.5",
    "awscred": "1.5.0",
    "chance": "1.1.11",
    "cheerio": "1.0.0-rc.12",
    "cross-env": "7.0.3",
    "cypress": "12.17.0",
    "cypress-data-session": "2.7.0",
    "cypress-mailosaur": "2.13.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.13.2",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "serverless": "3.33.0",
    "serverless-api-gateway-throttling": "2.0.3",
    "serverless-export-env": "2.2.0"
  },
  "dependencies": {
    "aws4": "1.12.0",
    "axios": "1.4.0",
    "lodash": "4.17.21",
    "mustache": "4.2.0"
  }
}
